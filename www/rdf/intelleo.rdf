
<!DOCTYPE rdf:RDF [
    <!ENTITY ns9 "http://rdfs.org/sioc/ns#" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY ns5 "http://www.intelleo.eu/ontologies/quiz/ns/" >
    <!ENTITY ns10 "http://intelleo.eu/ontologies/workflow/ns/" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY ns3 "http://www.intelleo.eu/ontologies/workflow/ns/" >
    <!ENTITY ns6 "http://www.intelleo.eu/ontologies/user-model/ns/" >
    <!ENTITY ns "http://www.intelleo.eu/ontologies/activities/ns#" >
    <!ENTITY ns7 "http://www.intelleo.eu/ontologies/annotations/ns/" >
    <!ENTITY ns8 "http://www.intelleo.eu/ontologies/competences/ns/" >
    <!ENTITY ns2 "http://www.intelleo.eu/ontologies/organization/ns/" >
    <!ENTITY ns4 "http://jelenajovanovic.net/ontologies/loco/alocom-core/ns/" >
]>


<rdf:RDF xmlns="http://www.intelleo.eu/ontologies/activities/ns#"
     xml:base="http://www.intelleo.eu/ontologies/activities/ns"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.intelleo.eu/ontologies/activities/ns#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:ns9="http://rdfs.org/sioc/ns#"
     xmlns:ns5="http://www.intelleo.eu/ontologies/quiz/ns/"
     xmlns:ns6="http://www.intelleo.eu/ontologies/user-model/ns/"
     xmlns:ns10="http://intelleo.eu/ontologies/workflow/ns/"
     xmlns:ns7="http://www.intelleo.eu/ontologies/annotations/ns/"
     xmlns:ns8="http://www.intelleo.eu/ontologies/competences/ns/"
     xmlns:ns2="http://www.intelleo.eu/ontologies/organization/ns/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ns4="http://jelenajovanovic.net/ontologies/loco/alocom-core/ns/"
     xmlns:ns3="http://www.intelleo.eu/ontologies/workflow/ns/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://www.intelleo.eu/ontologies/activities/ns">
        <dc:date xml:lang="en">2011-08-25</dc:date>
        <dc:title xml:lang="en">Activities Ontology</dc:title>
        <dc:creator xml:lang="en">Jelena Jovanovic</dc:creator>
        <dc:description xml:lang="en">Ontology for representing differen kinds of working and learning activities in Intelligent Extended Learning Organizations (IntelLEOs).</dc:description>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#loggingStateType -->

    <rdfs:Datatype rdf:about="http://www.intelleo.eu/ontologies/activities/ns#loggingStateType">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">loggedIn</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">loggedOut</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#subscriptionStateType -->

    <rdfs:Datatype rdf:about="http://www.intelleo.eu/ontologies/activities/ns#subscriptionStateType">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">turnedOff</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">turnedOn</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#achievedBy -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#achievedBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">achieved by</rdfs:label>
        <rdfs:comment xml:lang="en">The user who made the achievement (e.g., acquired a competence or reached a learning goal) </rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#AchievementEvent"/>
        <rdfs:subPropertyOf rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#performedBy"/>
        <rdfs:range rdf:resource="&ns6;User"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#achievement -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#achievement">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">achievement</rdfs:label>
        <rdfs:comment xml:lang="en">The property relates the a:AchievementEvent with the resource (e.g., a competence, a learning goal) that represent the user’s achievement</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#AchievementEvent"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#addedTo -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#addedTo">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">addedTo</rdfs:label>
        <rdfs:comment xml:lang="en">The resource to which another resource was added to (during the Add event); for example, a competence was added to a learning goal, or an activity is added to a learning path</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Add"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#annotationRef -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#annotationRef">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">annotationRef</rdfs:label>
        <rdfs:comment xml:lang="en">The property establishes a relation between an annotation event and the annotation that was generated during the event.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#AnnotationEvent"/>
        <rdfs:range rdf:resource="&ns7;Annotation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#collaborationMode -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#collaborationMode">
        <rdfs:label xml:lang="en">collaborationMode</rdfs:label>
        <rdfs:comment xml:lang="en">The mode of collaboration supported by an online environment; possible values include: Synchronous, Asynchronous, Multi-synchronous</rdfs:comment>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#CollaborationMode"/>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Environment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#contentRef -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#contentRef">
        <rdfs:label xml:lang="en">contentRef</rdfs:label>
        <rdfs:comment xml:lang="en">The content that was created/used/reaused/shared/... during an activity</rdfs:comment>
        <rdfs:range rdf:resource="&ns4;ContentUnit"/>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#detachedFrom -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#detachedFrom">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">detached from</rdfs:label>
        <rdfs:comment xml:lang="en">The resource from which another resource was removed (during the Detach event); for example, a competence was removed from a learning goal, or an activity was removed from a learning path</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#DetachEvent"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#exchangedMessage -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#exchangedMessage">
        <rdfs:label xml:lang="en">exchangedMessage</rdfs:label>
        <rdfs:comment xml:lang="en">The message that was exchanged during a discussion activity or a discussion event.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Message"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.intelleo.eu/ontologies/activities/ns#Discussing"/>
                    <rdf:Description rdf:about="http://www.intelleo.eu/ontologies/activities/ns#DiscussionEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#hasAttachment -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#hasAttachment">
        <rdfs:label xml:lang="en">hasAttachment</rdfs:label>
        <rdfs:comment xml:lang="en">The attachment to a message exchanged in communication channel</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Message"/>
        <rdfs:range rdf:resource="&foaf;Document"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#hasEvent -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#hasEvent">
        <rdfs:label xml:lang="en">hasEvent</rdfs:label>
        <rdfs:comment xml:lang="en">The property estableshes a relation between an activity and an its event(s).</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Activity"/>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#helpForResource -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#helpForResource">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">help for resource</rdfs:label>
        <rdfs:comment xml:lang="en">The resource that the given help request refers to; it can be a competence, an activity or a knowledge asset</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#HelpRequest"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#helpRequest -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#helpRequest">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">help request</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates the event of submitting a request for help with the actual request that was submitted.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#HelpRequest"/>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#HelpRequestEvent"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#inChannel -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#inChannel">
        <rdfs:label xml:lang="en">inChannel</rdfs:label>
        <rdfs:comment xml:lang="en">The property relates a message with the communication channel it was submitted to/receaved from.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Channel"/>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Message"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#joinedGroup -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#joinedGroup">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">joined group</rdfs:label>
        <rdfs:comment xml:lang="en">The relationship between the event of joining a group and the actual group that the user has joined.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#JoinGroupEvent"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&foaf;Group"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#leftGroup -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#leftGroup">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">left group</rdfs:label>
        <rdfs:comment xml:lang="en">The relationship between the event of (someone) leaving a group and the actual group that someone has left.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#LeaveGroupEvent"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&foaf;Group"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#occursIn -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#occursIn">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">The online environment where the activity has taken place</rdfs:label>
        <rdfs:label xml:lang="en">occursIn</rdfs:label>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Activity"/>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Environment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#participant -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#participant">
        <rdfs:label xml:lang="en">participant</rdfs:label>
        <rdfs:comment xml:lang="en">The property estableshes a relationship between an online discussion and its participants.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Discussing"/>
        <rdfs:range rdf:resource="&ns6;User"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#performedBy -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#performedBy">
        <rdfs:label xml:lang="en">performedBy</rdfs:label>
        <rdfs:comment xml:lang="en">The Agent who took the part in the given Event</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Event"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#progressScaleUsed -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#progressScaleUsed">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">progressScaleUsed</rdfs:label>
        <rdfs:comment xml:lang="en">The scale used for measuring and representing the user’s progress.</rdfs:comment>
        <rdfs:range rdf:resource="&ns10;ProgressScale"/>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#ChangeProgressEvent"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#quizResult -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#quizResult">
        <rdfs:label xml:lang="en">quizResult</rdfs:label>
        <rdfs:comment xml:lang="en">The result of the quizzing (assessment) activity</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Quizzing"/>
        <rdfs:range rdf:resource="&ns5;QuizResult"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#recommendedBy -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#recommendedBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">recommended by</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates the recommendation with the user who has made/given it.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Recommendation"/>
        <rdfs:range rdf:resource="&ns6;User"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#recommendedResource -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#recommendedResource">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">recommended resource</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates the recommendation with the resource (e.g., a learning goal, competence, activity, etc) that was recommended</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Recommendation"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#recommendedTo -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#recommendedTo">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">recommended to</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates the recommendation with the user whom it was intended for (i.e., to whom the resource was recommended)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Recommendation"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#relatedToGoal -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#relatedToGoal">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">related to goal</rdfs:label>
        <rdfs:comment xml:lang="en">The learning goal that the given help request is related to</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#HelpRequest"/>
        <rdfs:range rdf:resource="&ns6;LearningGoal"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#resourceRef -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#resourceRef">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">resourceRef</rdfs:label>
        <rdfs:comment xml:lang="en">Reference to the resource that was affected by or resulted from the given event</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Event"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#responseToRequest -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#responseToRequest">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">response to request</rdfs:label>
        <rdfs:comment xml:lang="en">The response that someone has submitted to the given help request.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#HelpRequest"/>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#HelpResponse"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#rootPosting -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#rootPosting">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">root post</rdfs:label>
        <rdfs:comment xml:lang="en">The property which establishes a relationship between a post (i.e. an asynchronous message) and the root post (of the discussion thread) this one refers to.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#ForumPost"/>
        <rdfs:range rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#ForumPost"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#sentBy -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#sentBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">sent by</rdfs:label>
        <rdfs:comment xml:lang="en">The property establishes a relationship between the meassage (synchronous or asynchronous) that was sent and the user who has sent it.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.intelleo.eu/ontologies/activities/ns#Message"/>
        <rdfs:range rdf:resource="&ns6;User"/>
    </owl:ObjectProperty>
    


    <!-- http://www.intelleo.eu/ontologies/activities/ns#sharedResource -->

    <owl:ObjectProperty rdf:about="http://www.intelleo.eu/ontologies/activities/ns#sharedResource">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
